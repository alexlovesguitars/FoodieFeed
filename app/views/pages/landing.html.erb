<div class="carousel-container">
  <div class="carousel-inner">
    <div id="carouselExampleSlidesOnly" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active">
          <%= image_tag "https://res.cloudinary.com/dmj0udbma/image/upload/v1742660478/Influencer_2_klqeks.jpg", class: "d-block w-100", alt: "Influencer" %>
        </div>
        <div class="carousel-item">
          <%= image_tag "https://res.cloudinary.com/dmj0udbma/image/upload/v1742660483/Pasta_byHand_lflmco.jpg", class: "d-block w-100", alt: "Pasta" %>
        </div>
        <div class="carousel-item">
          <%= image_tag "https://res.cloudinary.com/dmj0udbma/image/upload/v1742660477/Streetfood_Vietnam_lj9gny.jpg", class: "d-block w-100", alt: "Streetfood" %>
        </div>
        <div class="carousel-item">
          <%= image_tag "https://res.cloudinary.com/dmj0udbma/image/upload/v1742660483/SteamingKitchen_k80nj7.jpg", class: "d-block w-100", alt: "Kitchen" %>
        </div>
      </div>
    </div>
  </div>
  <%= image_tag "FoodieFeedLogo.svg", class: "image-overlay" %>
  <div class="buttons d-flex justify-content-center">
    <%= link_to "Sign Up!", new_user_registration_path, class: "btn btn-primary rounded-pill text-white image-overlay-button-1 px-3" %>
    <%= link_to "Sign In!", user_session_path, class: "btn btn-primary rounded-pill text-white image-overlay-button-2 px-3" %>
  </div>
</div>
